<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ request.static_url('blog_pyramid:static/assets/images/favicon.png') }}">
    <title>Elite Admin Template - The Ultimate Multipurpose admin template</title>
    <link href="{{ request.static_url('blog_pyramid:static/theme.css')}}" rel="stylesheet">
    <link href="{{ request.static_url('blog_pyramid:static/css/style.min.css')}}" rel="stylesheet">

    <link href="{{ request.static_url('blog_pyramid:static/assets/node_modules/footable/css/footable.bootstrap.min.css') }}" rel="stylesheet">


    <link href="{{ request.static_url('blog_pyramid:static/css/pages/footable-page.css" rel="stylesheet') }}">
    <link href="{{ request.static_url('blog_pyramid:static/css/pages/other-pages.css" rel="stylesheet') }}">


</head>

<body class="skin-default fixed-layout">

    <div id="main-wrapper">
        {% include 'header.html' %}
        {% include 'sidebar.html' %}

        <div class="page-wrapper">

            <div class="container-fluid">
                {% block content %}
                {% endblock %}
            </div>
        </div>

    </div>

    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/jquery/jquery-3.2.1.min.js') }}"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/popper/popper.min.js') }}"></script>
    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="{{ request.static_url('blog_pyramid:static/js/perfect-scrollbar.jquery.min.js') }}"></script>
    <!--Wave Effects -->
    <script src="{{ request.static_url('blog_pyramid:static/js/waves.js') }}"></script>
    <!--Menu sidebar -->
    <script src="{{ request.static_url('blog_pyramid:static/js/sidebarmenu.js') }}"></script>
    <!--stickey kit -->
    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/sticky-kit-master/dist/sticky-kit.min.js') }}"></script>
    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/sparkline/jquery.sparkline.min.js') }}"></script>

    <!--Custom JavaScript -->
    <script src="{{ request.static_url('blog_pyramid:static/js/custom.min.js') }}"></script>

    <!-- Footable -->
    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/moment/moment.js') }}"></script>
    <script src="{{ request.static_url('blog_pyramid:static/assets/node_modules/footable/js/footable.min.js') }}"></script>
    <!--FooTable init-->
    <script src="{{ request.static_url('blog_pyramid:static/dist/js/pages/footable-init.js') }}"></script>
</body>
</html>